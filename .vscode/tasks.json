{
 "version": "2.0.0",
 "presentation": {
  "echo": false,
  "reveal": "always",
  "focus": false,
  "panel": "dedicated",
  "showReuseMessage": true
 },
 "tasks": [
  {
   "label": "Project Startup",
   "dependsOn": [
    "src-compile",

   ],
   // Mark as the default build task so cmd/ctrl+shift+b will create them
   "group": {
    "kind": "build",
    "isDefault": true
   },
   // Try start the task on folder open
   "runOptions": {
    "runOn": "folderOpen"
   }
  },
  {
   "label": "src-compile",
   "type": "shell",
   "command": "cd src; tsc --watch",
   "isBackground": true
  },

  {
   "label": "dsst-libs",
   "type": "shell",
   "command": "dsst -a -c .libdsconfig",
   "isBackground": true
  }
 ]
}
